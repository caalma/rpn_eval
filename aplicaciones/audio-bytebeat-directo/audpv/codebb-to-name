#!/usr/bin/env python3
# -*- coding:utf-8 -*-

from sys import argv


def convert_codbb_to_name(text):
    dat = text.split()
    ree = {
        '>>': 'i',
        '<<': 'd',
        '&': 'y',
        '|': 'o',
        '^': 'x',
        '~': 'n',
        '+': 'a',
        '-': 's',
        '*': 'p',
        '/': 'c',
        '%': 'r',
        't': 't',
        '&&': 'f',
        '||': 'g',
        '==': 'h',
        '!=': 'j',
        '<=': 'k',
        '>=': 'l',
        '<': 'v',
        '>': 'w',
        # bemquz
        }
    result = []
    digit_previus = False
    for v in dat:
        if v in ree:
            result.append(ree[v])
            digit_previus = False
        elif v.isdigit():
            if digit_previus:
                result.append('_')
            result.append(v)
            digit_previus = True
        else:
            digit_previus = False
            result.append(f'_{v}_')

    return ''.join(result)



if __name__ == "__main__":
    if len(argv) > 1:
        print(convert_codbb_to_name(argv[1]))
